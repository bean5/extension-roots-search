{
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ 
      "jquery.js", 
      "fsTreeSearch.js",
      "widgets/fs-familytree-ancestor-search.js",
      "builders/familysearch.js",
      "builders/ancestry.js",
      "builders/werelate.js"
    ],
    "matches": [ "https://familysearch.org/*", "https://www.familysearch.org/*" ],
    "run_at": "document_end",
		"css": [ "fsTreeSearch.css" ]
  } ],
  "description": "In the ancestor view, add search links to popular genealogy search engines with the names, places, and dates for the ancestor automatically filled in.",
  "name": "FamilySearch Family Tree Enhancements",
  "permissions": [ "https://familysearch.org/*", "https://www.familysearch.org/*" ],
  "version": "1.2"
}
